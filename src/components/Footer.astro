<style>
  footer {
    display: flex;
    gap: 1rem;
    justify-content: space-between;
    list-style-type: none;
    background: rgb(254, 132, 47);
    left: 50%;
    margin-left: -50vw;
    margin-right: -50vw;
    max-width: 100vw;
    position: relative;
    right: 50%;
    width: 100vw;
    padding-left: 10px;
    padding-right: 10px;
    color: white;
    font-size: 1rem;
  }

  footer a {
    color: white;
    text-decoration: none;
    text-transform: uppercase;
  }

  .social:hover path {
    fill: var(--site-orange);
  }

  h2 {
    font-size: 1.875rem;
    line-height: 2.25rem;
    font-weight: 800;
    text-align: center;
  }

  .mt-8 {
    margin-top: 2rem;
  }

  .mx-3 {
    margin-left: 0.75rem;
    margin-right: 0.75rem;
  }
  .social-container {
    display: flex;
    justify-content: space-between;
  }
  .social {
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
  }
  .social svg {
    width: 50px;
    height: 50px;
  }

  .social-text {
    padding-top: 0.25rem;
    font-weight: 700;
    opacity: 0;
  }

  .pb-8 {
    padding-bottom: 2rem;
  }
</style>

